@using EntityLayer.Concrete
@model List<Destinational>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_MemberLayout.cshtml";
}

<h1>Aktif Tur Rotalarımız</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Şehir</th>
        <th>Süre</th>
        <th>Kapasite</th>
        <th>Fiyat</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.DestinationId</td>
            <td>@item.DestinationCity</td>
            <td>@item.DestinationDayNight</td>
            <td>@item.DestinationCapacity</td>
            <td>@item.DestinationPrice ₺</td>
            <td> <a href="#" class="btn btn-outline-info">Detaylar</a> </td>
        </tr>
    }
</table>

